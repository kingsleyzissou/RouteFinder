<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.Slider?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="720.0" prefWidth="1280.0" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.got.app.SceneController">
    <ScrollPane fx:id="scrollPane" prefHeight="720.0" prefWidth="1100.0">
        <Pane>
            <ImageView fx:id="image">
                <image>
                    <Image url="@got.jpg" />
                </image>
            </ImageView>
            <Pane fx:id="pane" />
        </Pane>
    </ScrollPane>
   <VBox layoutX="1101.0" prefHeight="410.0" prefWidth="179.0">
      <children>
          <Slider fx:id="slider" blockIncrement="0.1" max="1.5" min="0.2161" value="0.2161">
            <VBox.margin>
               <Insets bottom="10.0" top="10.0" />
            </VBox.margin>
         </Slider>
         <Label text="From">
            <VBox.margin>
               <Insets top="10.0" />
            </VBox.margin>
         </Label>
         <ComboBox fx:id="from" prefHeight="27.0" prefWidth="179.0">
            <VBox.margin>
               <Insets bottom="15.0" />
            </VBox.margin>
         </ComboBox>
         <Label text="To" />
         <ComboBox fx:id="to" prefHeight="27.0" prefWidth="180.0">
            <VBox.margin>
               <Insets bottom="15.0" />
            </VBox.margin>
         </ComboBox>
         <Label text="Waypoints" />
         <ComboBox prefHeight="27.0" prefWidth="182.0">
            <VBox.margin>
               <Insets bottom="15.0" />
            </VBox.margin>
         </ComboBox>
         <Label text="Avoid towns" />
         <ComboBox prefHeight="27.0" prefWidth="179.0">
            <VBox.margin>
               <Insets bottom="15.0" />
            </VBox.margin>
         </ComboBox>
         <Button mnemonicParsing="false" text="Go">
            <VBox.margin>
               <Insets top="10.0" />
            </VBox.margin>
         </Button>
      </children>
   </VBox>
</AnchorPane>
